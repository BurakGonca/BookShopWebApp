<!DOCTYPE html>
<html class="no-js">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>@ViewBag.Title - mPurpose</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width">

    <link rel="stylesheet" href="~/bookshop/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/bookshop/css/icomoon-social.css">
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,700,600,800' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="~/bookshop/css/leaflet.css" />
    <link rel="stylesheet" href="~/bookshop/css/main.css">

    <script src="~/bookshop/js/modernizr-2.6.2-respond-1.1.0.min.js"></script>
</head>
<body>
    <div class="mainmenu-wrapper">
        <div class="container">
            <div class="menuextras">
                <div class="extras">
                    <ul>
                        <li class="shopping-cart-items"><i class="glyphicon glyphicon-shopping-cart icon-white"></i> <a href="~/bookshop/page-shopping-cart.html"><b>3 items</b></a></li>
                        <li>
                            <div class="dropdown choose-country">
                                <a class="#" data-toggle="dropdown" href="#"><img src="~/bookshop/img/flags/gb.png" alt="Great Britain"> UK</a>
                                <ul class="dropdown-menu" role="menu">
                                    <li role="menuitem"><a href="#"><img src="~/bookshop/img/flags/us.png" alt="United States"> US</a></li>
                                    <li role="menuitem"><a href="#"><img src="~/bookshop/img/flags/de.png" alt="Germany"> DE</a></li>
                                    <li role="menuitem"><a href="#"><img src="~/bookshop/img/flags/es.png" alt="Spain"> ES</a></li>
                                </ul>
                            </div>
                        </li>
                        <li><a href="~/bookshop/page-login.html">Login</a></li>
                    </ul>
                </div>
            </div>
            <nav id="mainmenu" class="mainmenu">
                <ul>
                    <li class="logo-wrapper"><a href="~/bookshop/index.html"><img src="~/bookshop/img/mPurpose-logo.png" alt="Multipurpose Twitter Bootstrap Template"></a></li>
                    <li class="active"><a href="~/bookshop/index.html">Home</a></li>
                    <li><a href="~/bookshop/features.html">Features</a></li>
                    <li class="has-submenu">
                        <a href="#">Pages +</a>
                        <div class="mainmenu-submenu">
                            <div class="mainmenu-submenu-inner">
                                <div>
                                    <h4>Homepage</h4>
                                    <ul>
                                        <li><a href="~/bookshop/index.html">Homepage (Sample 1)</a></li>
                                        <li><a href="~/bookshop/page-homepage-sample.html">Homepage (Sample 2)</a></li>
                                    </ul>
                                    <h4>Services & Pricing</h4>
                                    <ul>
                                        <li><a href="~/bookshop/page-services-1-column.html">Services/Features (Rows)</a></li>
                                        <li><a href="~/bookshop/page-services-3-columns.html">Services/Features (3 Columns)</a></li>
                                        <li><a href="~/bookshop/page-services-4-columns.html">Services/Features (4 Columns)</a></li>
                                        <li><a href="~/bookshop/page-pricing.html">Pricing Table</a></li>
                                    </ul>
                                    <h4>Team & Open Vacancies</h4>
                                    <ul>
                                        <li><a href="~/bookshop/page-team.html">Our Team</a></li>
                                        <li><a href="~/bookshop/page-vacancies.html">Open Vacancies (List)</a></li>
                                        <li><a href="~/bookshop/page-job-details.html">Open Vacancies (Job Details)</a></li>
                                    </ul>
                                </div>
                                <div>
                                    <h4>Our Work (Portfolio)</h4>
                                    <ul>
                                        <li><a href="~/bookshop/page-portfolio-2-columns-1.html">Portfolio (2 Columns, Option 1)</a></li>
                                        <li><a href="~/bookshop/page-portfolio-2-columns-2.html">Portfolio (2 Columns, Option 2)</a></li>
                                        <li><a href="~/bookshop/page-portfolio-3-columns-1.html">Portfolio (3 Columns, Option 1)</a></li>
                                        <li><a href="~/bookshop/page-portfolio-3-columns-2.html">Portfolio (3 Columns, Option 2)</a></li>
                                        <li><a href="~/bookshop/page-portfolio-item.html">Portfolio Item (Project) Description</a></li>
                                    </ul>
                                    <h4>General Pages</h4>
                                    <ul>
                                        <li><a href="~/bookshop/page-about-us.html">About Us</a></li>
                                        <li><a href="~/bookshop/page-contact-us.html">Contact Us</a></li>
                                        <li><a href="~/bookshop/page-faq.html">Frequently Asked Questions</a></li>
                                        <li><a href="~/bookshop/page-testimonials-clients.html">Testimonials & Clients</a></li>
                                        <li><a href="~/bookshop/page-events.html">Events</a></li>
                                        <li><a href="~/bookshop/page-404.html">404 Page</a></li>
                                        <li><a href="~/bookshop/page-sitemap.html">Sitemap</a></li>
                                        <li><a href="~/bookshop/page-login.html">Login</a></li>
                                        <li><a href="~/bookshop/page-register.html">Register</a></li>
                                        <li><a href="~/bookshop/page-password-reset.html">Password Reset</a></li>
                                        <li><a href="~/bookshop/page-terms-privacy.html">Terms & Privacy</a></li>
                                        <li><a href="~/bookshop/page-coming-soon.html">Coming Soon</a></li>
                                    </ul>
                                </div>
                                <div>
                                    <h4>eShop</h4>
                                    <ul>
                                        <li><a href="~/bookshop/page-products-3-columns.html">Products listing (3 Columns)</a></li>
                                        <li><a href="~/bookshop/page-products-4-columns.html">Products listing (4 Columns)</a></li>
                                        <li><a href="~/bookshop/page-products-slider.html">Products Slider</a></li>
                                        <li><a href="~/bookshop/page-product-details.html">Product Details</a></li>
                                        <li><a href="~/bookshop/page-shopping-cart.html">Shopping Cart</a></li>
                                    </ul>
                                    <h4>Blog</h4>
                                    <ul>
                                        <li><a href="~/bookshop/page-blog-posts.html">Blog Posts (List)</a></li>
                                        <li><a href="~/bookshop/page-blog-post-right-sidebar.html">Blog Single Post (Right Sidebar)</a></li>
                                        <li><a href="~/bookshop/page-blog-post-left-sidebar.html">Blog Single Post (Left Sidebar)</a></li>
                                        <li><a href="~/bookshop/page-news.html">Latest & Featured News</a></li>
                                    </ul>
                                </div>
                            </div><!-- /mainmenu-submenu-inner -->
                        </div><!-- /mainmenu-submenu -->
                    </li>
                    <li><a href="~/bookshop/credits.html">Credits</a></li>
                </ul>
            </nav>
        </div>
    </div>

    <div class="homepage-slider">
        <div id="sequence">
            <ul class="sequence-canvas">
                <li class="bg4">
                    <h2 class="title">Responsive</h2>
                    <h3 class="subtitle">It looks great on desktops, laptops, tablets and smartphones</h3>
                    <img class="slide-img" src="~/bookshop/img/homepage-slider/slide1.png" alt="Slide 1" />
                </li>
                <li class="bg3">
                    <h2 class="title">Color Schemes</h2>
                    <h3 class="subtitle">Comes with 5 color schemes and it's easy to make your own!</h3>
                    <img class="slide-img" src="~/bookshop/img/homepage-slider/slide2.png" alt="Slide 2" />
                </li>
                <li class="bg1">
                    <h2 class="title">Feature Rich</h2>
                    <h3 class="subtitle">Huge amount of components and over 30 sample pages!</h3>
                    <img class="slide-img" src="~/bookshop/img/homepage-slider/slide3.png" alt="Slide 3" />
                </li>
            </ul>
            <div class="sequence-pagination-wrapper">
                <ul class="sequence-pagination">
                    <li>1</li>
                    <li>2</li>
                    <li>3</li>
                </ul>
            </div>
        </div>
    </div>

    @RenderBody()

    <div class="footer">
        <div class="container">
            <div class="row">
                <div class="col-footer col-md-3 col-xs-6">
                    <h3>Our Latest Work</h3>
                    <div class="portfolio-item">
                        <div class="portfolio-image">
                            <a href="~/bookshop/page-portfolio-item.html"><img src="~/bookshop/img/portfolio6.jpg" alt="Project Name"></a>
                        </div>
                    </div>
                </div>
                <div class="col-footer col-md-3 col-xs-6">
                    <h3>Navigate</h3>
                    <ul class="no-list-style footer-navigate-section">
                        <li><a href="~/bookshop/page-blog-posts.html">Blog</a></li>
                        <li><a href="~/bookshop/page-portfolio-3-columns-2.html">Portfolio</a></li>
                        <li><a href="~/bookshop/page-products-3-columns.html">eShop</a></li>
                        <li><a href="~/bookshop/page-services-3-columns.html">Services</a></li>
                        <li><a href="~/bookshop/page-pricing.html">Pricing</a></li>
                    </ul>
                </div>
                <div class="col-footer col-md-3 col-xs-6">
                    <h3>Contacts</h3>
                    <p class="contact-us-details">
                        <b>Address:</b> 123 Fake Street, LN1 2ST, London, United Kingdom<br />
                        <b>Phone:</b> +44 123 654321<br />
                        <b>Fax:</b> +44 123 654321<br />
                        <b>Email:</b> <a href="mailto:getintouch@yourcompanydomain.com">getintouch@yourcompanydomain.com</a>
                    </p>
                </div>
                <div class="col-footer col-md-3 col-xs-6">
                    <h3>About Our Company</h3>
                    <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Curabitur euismod enim a metus adipiscing aliquam.</p>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="footer-copyright">&copy; 2013 mPurpose. All rights reserved.</div>
                </div>
            </div>
        </div>
    </div>

    <script src="~/bookshop/js/jquery-1.9.1.min.js"></script>
    <script src="~/bookshop/js/bootstrap.min.js"></script>
    <script src="~/bookshop/js/leaflet.js"></script>
    <script src="~/bookshop/js/jquery.sequence-min.js"></script>
    <script src="~/bookshop/js/jquery.bxslider.js"></script>
    <script src="~/bookshop/js/main-menu.js"></script>
    <script src="~/bookshop/js/template.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
